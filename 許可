    warn("ãƒ‡ã‚³ãƒ¼ãƒ‰å¤±æ•—:", decodeErr)
    delay(3, function()
        localPlayer:Kick("ã‚ãªãŸã¯æœ¬å½“ã«é¦¬é¹¿ã§ã™ã­")
