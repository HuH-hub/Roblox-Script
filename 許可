local isWhitelisted = false
if whitelistData and whitelistData.Whitelist then
    for _, id in ipairs(whitelistData.Whitelist) do
        if tonumber(id) == localPlayer.UserId then
            isWhitelisted = true
            break
        end
    end
end

-- ãƒ›ãƒ¯ã‚¤ãƒˆãƒªã‚¹ãƒˆå†…ãªã‚‰ã“ã“ã§çµ‚äº†
